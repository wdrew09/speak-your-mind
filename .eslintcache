 be verified then go to posts page\n    useEffect(() => {\n        axiosInstance.get('account/verify?token=' + props.token)\n            .then(response => {\n                if (response.data.success) {\n                    console.log('go to post')\n                    setGoToPosts(true)\n                } else {\n                    console.log('error verifying account')\n                }\n            });\n    }, [props.token])\n\n\n    const submit = () => {\n        if (username && password) {\n            props.login(username, password)\n        }\n    }\n\n    return (\n        <div>\n            {goToPosts && <Redirect to='/posts' />}\n            login\n            username\n            <input type=\"text\" onChange={(e) => setUsername(e.target.value)} />\n            <br />\n            <br />\n            <br />\n            password\n            <input type=\"password\" onChange={(e) => setPassword(e.target.value)} />\n            <br />\n            <br />\n            <br />\n            <button onClick={() => submit()} >login</button>\n        </div>\n    )\n}\n\nconst mapStateToProps = (state) => {\n    return {\n        token: state.auth.token\n    };\n}\n\nconst mapDispatchToProps = (dispatch) => {\n    return {\n        login: (username, password) => dispatch(actionCreators.login(username, password))\n    }\n}\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Login);\n",["109","110"],"/Users/macbookpro/PersonalProjects/speakyourmind/src/utils/storage.js",[],"/Users/macbookpro/PersonalProjects/speakyourmind/src/Components/accountView.js",[],["111","112"],"/Users/macbookpro/PersonalProjects/speakyourmind/src/store/reducers/auth.js",[],"/Users/macbookpro/PersonalProjects/speakyourmind/src/store/actions/actionTypes.js",[],"/Users/macbookpro/PersonalProjects/speakyourmind/src/store/actions/auth.js",[],"/Users/macbookpro/PersonalProjects/speakyourmind/src/store/utility.js",[],"/Users/macbookpro/PersonalProjects/speakyourmind/src/store/actio