r navbar-dark bg-dark navbar-expand-lg\">\n                {goToLogin && <Redirect to='/' />}\n                <ToastContainer\n                    position=\"top-center\"\n                    autoClose={5000}\n                    hideProgressBar={false}\n                    newestOnTop={false}\n                    closeOnClick\n                    rtl={false}\n                    pauseOnFocusLoss\n                    draggable\n                    pauseOnHover\n                />\n                <Link to=\"/\" className=\"navbar-brand\">a</Link>\n                <div className=\"collpase navbar-collapse\">\n                    <ul className=\"navbar-nav mr-auto\">\n                        <li className=\"navbar-item\">\n                            <Link to=\"/account\" className=\"nav-link\">account</Link>\n                        </li>\n                        <li className=\"navbar-item\">\n                            <Link to=\"/posts\" className=\"nav-link\">posts</Link>\n                        </li>\n                        <li className=\"navbar-item\">\n                            <Link to=\"/create-post\" className=\"nav-link\">create post</Link>\n                        </li>\n                        <li className=\"navbar-item\">\n                            <button style={{ height: '50px', width: '100px', color: 'blue' }} onClick={() => onLogout()}>Logout</button>\n                        </li>\n                        {/* <li className=\"navbar-item\">\n                            <button style={{ height: '50px', width: '100px', color: 'blue' }} onClick={() => notify()}>notify</button>\n                        </li> */}\n                    </ul>\n                </div>\n            </nav>\n\n        );\n    } else {\n        return (<div />)\n    }\n\n}\n\nconst mapStateToProps = (state) => ({\n    token: state.auth.token,\n    message: state.alert.message,\n    style: state.alert.style\n});\n\n\nconst mapDispatchToProps = (dispatch) => {\n    return {\n        logout: () => dispatch(actionCreators.logout()),\n        setAlert: (message, style) => dispatch(actionCreators.setAlert(message, style))\n    }\n}\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Navbar);","/Users/macbookpro/PersonalProjects/speakyourmind/client/src/Components/accountView.js",["110","111"],"import React from 'react'\nimport { axiosInstance } from '../index';\n\nimport { connect } from 'react-redux';\nimport * as actionCreators from '../store/actions/index';\n\nconst AccountView = props => {\n    return (\n        <div>\n            {props.username}\n        </div>\n    )\n}\n\nconst mapStateToProps = (state) => {\n    return {\n        username: state.auth.username\n    };\n}\n\nconst mapDispatchToProps = (dispatch) => {\n    return {\n    }\n}\n\nexport default connect(mapStateToProps, mapDispatchToProps)(AccountView);\n","/Users/macbookpro/PersonalProjects/speakyourmind/client/src/Components/createPost.js",["112","113"],"import React, { useState } from 'react'\nimport { Link, Redirect } from 'react-router-dom';\nimport { axiosInstance } from '../index';\n\nimport { connect } from 'react-redux';\nimport * as actionCreators from '../store/actions/index';\n\nconst CreatePost = (props) => {\n    const [title, setTitle] = useState()\n    const [content, setContent] = useState()\n\n    const submitPressed = () => {\n        if (title && content) {\n            console.log(title, content, props.userId, props.username)\n            axiosInstance.post('posts/add', {\n                title: title,\n                content: content,\n                userId: props.userId,\n                username: props.username\n            })\n                .then(response => {\n                    if (response.data.success) {\n                        props.setAlert('Post Added!', 'success')\n                    } else {\n                        props.setAlert('Error Adding post...', 'error')\n                    }\n                });\n        